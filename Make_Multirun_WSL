run8:
	sudo docker run $(DOCKER_EXTRAS) --gpus all \
	jsrl-corl python $(RUN_FILE) --horizon_fn variance --checkpoints_path checkpoints --env antmaze-large-diverse-v2 --normalize True --normalize_reward True --iql_deterministic False --beta 10 --iql_tau 0.9 --eval_freq 10000 --pretrained_policy_path jsrl-CORL/algorithms/finetune/checkpoints/IQL-antmaze-large-diverse-v2-offline/checkpoint_999999.pt --offline_iterations 0 ;
	sh check_stopped.sh ;

run9:
	sudo docker run $(DOCKER_EXTRAS) --gpus all \
	jsrl-corl python $(RUN_FILE) --horizon_fn variance --checkpoints_path checkpoints --env antmaze-large-diverse-v2 --normalize True --normalize_reward True --iql_deterministic False --beta 10 --iql_tau 0.9 --eval_freq 10000 --pretrained_policy_path jsrl-CORL/algorithms/finetune/checkpoints/IQL-antmaze-large-diverse-v2-offline/checkpoint_999999.pt --offline_iterations 0 ;
